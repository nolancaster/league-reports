<p id="notice"><%= notice %></p>

<h1>Lineups</h1>

<table>
  <thead>
    <tr>
      <th>Score</th>
      <th>Result</th>
      <th>Team name</th>
      <th>Team</th>
      <th>Owner</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lineups.each do |lineup| %>
      <tr>
        <td><%= lineup.score %></td>
        <td><%= lineup.result %></td>
        <td><%= lineup.team_name %></td>
        <td><%= lineup.team %></td>
        <td><%= lineup.owner %></td>
        <td><%= link_to 'Show', lineup %></td>
        <td><%= link_to 'Edit', edit_lineup_path(lineup) %></td>
        <td><%= link_to 'Destroy', lineup, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lineup', new_lineup_path %>
